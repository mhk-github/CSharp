<?xml version="1.0" encoding="utf-8" ?>
<configuration>

	<startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

	<appSettings>
		<add key="MainWindowTopLeftX" value="2" />
		<add key="MainWindowTopLeftY" value="2" />
		<add key="MainWindowWidth" value="500" />
		<add key="MainWindowHeight" value="245" />

		<add key="Recurse" value="true" />
		<add key="FilePatterns" value="*.jpg;*.jpeg;*.png;*.bmp" />

		<add key="LoggingConfiguration" value={LOG4NET.CONFIG} />
	</appSettings>

</configuration>
